{
    "name": "o3din",
    "version": "10.0.0",
    "description": "o3din - Advanced WhatsApp Bot",
    "type": "module",
    "license": "Apache-2.0",
    "author": {
        "name": "o3din",
        "url": "https://github.com/o3din"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/o3din/o3din.git"
    },
    "homepage": "https://github.com/o3din/o3din.git#readme",
    "bugs": {
        "url": "https://github.com/o3din/o3din.git/issues"
    },
    "engines": {
        "bun": ">=1.3.0"
    },
    "scripts": {
        "start": "bun run --smol src/main.js"
    },
    "dependencies": {
        "@napi-rs/canvas": "^0.1.84",
        "async-mutex": "^0.5.0",
        "baileys": "github:o3din/baileys",
        "eventemitter3": "^5.0.1",
        "file-type": "^21.3.0",
        "node-webpmux": "^3.2.1",
        "p-queue": "^9.1.0"
    },
    "imports": {
        "#api/*": "./src/lib/api/*",
        "#canvas/*": "./src/lib/canvas/*",
        "#auth": "./src/lib/core/auth/auth.js",
        "#core/*": "./src/lib/core/*",
        "#lib/*": "./src/lib/*"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "arrowParens": "always",
        "endOfLine": "lf",
        "proseWrap": "preserve",
        "htmlWhitespaceSensitivity": "css",
        "embeddedLanguageFormatting": "auto"
    },
    "trustedDependencies": [
        "baileys",
        "protobufjs",
        "sharp"
    ],
    "devDependencies": {
        "prettier": "^3.8.0"
    }
}